#access element at a particular index
#Array.at(n)
puts Arr1.at(3)

#to provide default array error if out of index
#Array.fetch(n)
#puts Arr1.fetch(8)
puts Arr1.fetch(3)

#return the first n elements of an array
#Array.take(n)
print Arr1.take(3)
puts

#add item to the end
#Array.push(obj)
#Array << (obj)
print Arr1.push(6)
puts
print Arr1 << (7)
puts

#add item at the begining
#Array.unshift(obj)
print Arr1.unshift(0)
puts

#add item at the specific location
#Array.insert(3,obj)
print Arr1.insert(4,4.5)
puts

#return elements after n elements have been dropped
#Array.drop(n)
print Arr1.drop(2)
puts

#remove from the end
#Array.pop
print Arr1.pop
puts
print Arr1
puts

#remove from begining
#Array.shift
print Arr1.shift
puts
print Arr1
puts

#remove item from any specific location
#Array.delect(obj)
print Arr1.delete(5)
puts
print Arr1
puts

#remove any duplicate items
#Array.uniq
print Arr1.uniq
puts
print Arr1
puts
